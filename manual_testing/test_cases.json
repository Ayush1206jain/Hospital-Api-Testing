      "testId": "HC-001",
      "endpoint": "/v1/hospitais/",
      "method": "POST",
      "category": "VALID",
      "description": "Create hospital with all required fields",
      "priority": "HIGH",
      "tags": ["hospital", "create", "valid"],
      "requestHeaders": {
        "Content-Type": "application/json"
      },
      "requestBody": {
        "id": "100",
        "name": "Hospital Central",
        "address": "Rua Principal, 123",
        "beds": 50,
        "availableBeds": 30,
        "location": {
          "referenceId": "Rua Principal, 123",
          "locationCategory": "HOSPITAL",
          "name": "Hospital Central",
          "latitude": -23.593438,
          "longitude": -46.710462
        }
      },
      "expectedStatusCode": 200,
      "expectedResponseFields": [
        "id",
        "name",
        "address",
        "beds",
        "availableBeds"
      ],
      "assertions": [
        "status == 200",
        "response.id == '100'",
        "response.name == 'Hospital Central'",
        "response.address == 'Rua Principal, 123'",
        "response.beds == 50"
      ]
    },
